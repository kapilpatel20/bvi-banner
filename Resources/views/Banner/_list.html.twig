{% if lstObj|length > 0 %}
{% for lst in lstObj %}    
    <tr bid="{{lst.id}}">
        <td>{{lst.id}}</td>
        <td>{{lst.title}}</td>
        <td><img style="max-width:100px" src="{{asset('./uploads/banners/'~lst.image)}}"/></td>
        <td>{{lst.status}}</td>
        <td>{{lst.sequence}}</td>
        <td>{{lst.createdat | format('Y-m-d')}}</td>
        <td><a href="{{ path('bvi_banner_edit', {'id' : lst.id})}}"> Edit</a> | <a href="javascript://" onclick="updateStatus('{{lst.id}}')">Change Status</a></td>
    </tr>
{% endfor %}
{% else %}
    <tr><td colspan="5">No record found</td></tr>
{% endif %}
<div class="navigation">
 {{ knp_pagination_render(lstObj) }}
</div>